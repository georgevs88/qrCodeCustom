									<textbox value="@{wAna.aenVO.riscoPos}" width="90px" style="text-align:center" readonly="true"/>
									<textbox value="@{wAna.aenVO.riscoTra}" width="90px" style="text-align:center" readonly="true"/>
									<textbox value="@{wAna.aenVO.riscoSia}" width="90px" style="text-align:center" readonly="true"/>
								</row>
							</rows>
						</grid>
					</hbox>
				</vbox>
			</groupbox>
			<groupbox width="930px">
				<vbox>
					<div style="background:#407DBA;padding:5px">
						<label value="Anexos" style="color:white;font-weight:bold;font-size:14px;" />
					</div>
					<grid align="center">
						<columns>
							<column label="" align="right" width="105px"/>
							<column label=""/>
						</columns>
						<rows>
							<row spans="2">
								<hbox pack="center" width="100%">
									<vbox>
										<label value="Arquivos aceitos: jpg, jpeg, png, bmp, doc, docx, xls, xlsx, xps, pps, odt, ods, pdf, zip, rar e txt." style="font-size:11px;color:red" />
										<label value="TAMANHO MÁXIMO DO ARQUIVO PERMITIDO PARA ANEXO: 100MB." style="font-size:13px;color:red" />
									</vbox>
								</hbox>
							</row>
							<row>
								<label value="Arquivo"/>
								<hbox>
									<textbox value="@{wAna.up.nomeArquivo}" width="432px" disabled="true"/>
									<separator width="15px"/>
									<button id="selArq" label="Selecionar Arquivo" upload="true" onUpload="wAna.upload(event)" />
								</hbox>
							</row>
							<row>
								<label value="Descrição"/>
								<textbox id="desarq" value="@{wAna.descricaoArquivo}" width="584px"/>
							</row>
							<row spans="2">
								<hbox pack="center" width="100%">
									<button class="btn btn-info" label="Adicionar" onClick="wAna.anexar()" />
									<button label="Cancelar" onClick="wAna.cancelarAnexo()" />
								</hbox>
							</row>
						</rows>
					</grid>
					<hbox pack="center" width="100%">
						<listbox width="622px" model="@{wAna.listAnx}" emptyMessage="Nenhum anexo encontrado.">
							<listhead>
								<listheader label="Usuário" align="center" />
								<listheader label="Descrição" />
								<listheader label="Arquivo" />
								<listheader width="30px" align="center" />
								<listheader width="30px" align="center" />
							</listhead>
							<listitem self="@{each=anex}" value="@{anex}">
								<listcell label="@{anex.usualt}" />
								<listcell label="@{anex.desanx}" />
								<listcell label="@{anex.nomanx}" />
								<listcell image="/images/down.png" onClick="wAna.download(self.parent.value)" tooltiptext="Download"/>
								<listcell image="/images/delete.png" onClick="wAna.deletarArquivo(self.parent.value)" tooltiptext="Excluir"/>	
							</listitem>
						</listbox>
					</hbox>
				</vbox>
			</groupbox>
			<groupbox width="930px">
				<vbox>
					<div style="background:#407DBA;padding:5px">
						<label value="Parecer" style="color:white;font-weight:bold;font-size:14px;" />
					</div>
					<grid align="right" width="930px">
						<columns>
							<column label="" align="right" width="105px"/>
							<column label="" width="105px" />
							<column label="" align="right" width="130px" />
							<column label="" />
						</columns>
						<rows>
							<row style="padding:1px;">
								<label value="Data da Análise:" style="font-size:11px;"/>
								<textbox readonly="true" value="@{wAna.dataAnalise}" style="font-size:11px;font-weight:bold;"/>
								<label value="Status:" style="font-size:12px;font-weight:bold;"/>
								<combobox id="comboStatus" readonly="true" width="120px" selectedItem="@{wAna.aenVO.sitaen}">
									<comboitem id="cb1" label="Deferido" value="D" style="font-size:11px;color:green"/>
									<comboitem id="cb2" label="Indeferido" value="I" style="font-size:11px;color:red"/>
								</combobox>
							</row>
							<row spans="1,3" style="padding:1px;">
								<label value="Embasamento Legal:" style="font-size:11px;"/>
								<checkbox id="checkEmb" checked="@{wAna.checkEmbasamento}" label="Nos termos da Lei de Ordenamento do Uso e Ocupação do Solo" style="font-size:11px;"/>
							</row>
							<row spans="4">
								<div align="left">
									<hbox>
										<separator width="96px"/>
										<combobox model="@{wAna.listMsgJust}" selectedItem="@{wAna.msgJust}" readonly="true" width="737px">
											<comboitem self="@{each=msg}" label="@{msg.destab}" style="font-size:11px"/>
										</combobox>
										<button label="Adicionar" onClick="wAna.addMsgObs()"/>
									</hbox>
								</div>
							</row>
							<row spans="1,3" style="padding:1px;">
								<label value="Observações:" style="font-size:11px;"/>
								<textbox id="observacoes" value="@{wAna.observacoes}" style="font-size:11px;" width="810px" maxlength="1000" rows="4"/>
							</row>
							<row spans="1,3" style="padding:1px;">
								<label value="Justificativa:" style="font-size:11px;"/>
								<textbox id="justificativa" value="@{wAna.justificativa}" style="font-size:11px;" width="810px" maxlength="1000" rows="4"/>
							</row>
<!-- 							<row spans="1,3" style="padding:1px;"> -->
<!-- 								<label value="Orientação:" style="font-size:11px;"/> -->
<!-- 								<textbox id="orientacao" value="@{wAna.orientacao}" style="font-size:11px;" width="810px" maxlength="1000" rows="4"/> -->
<!-- 							</row> -->
							<row spans="4">
								<div align="center">
									<button label="Emitir Parecer" onClick="wAna.salvar()" style="width: 200px;font-size: 16px;background-color: #00042A !important;color: white;font-weight: bold;height: 30px;border-radius: 7px !important;"/>
									<button label="Cancelar" onClick="wAna.limpar()" style="width: 100px;font-size: 12px;background-color: #00042A !important;color: white;height: 30px;border-radius: 7px !important;"/>
								</div>
							</row>
						</rows>
					</grid>
				</vbox>
			</groupbox>
		</vbox>
	</window>
</zk>
