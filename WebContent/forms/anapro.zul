				<groupbox width="902px">
					<div style="background:#407DBA;padding:5px">
						<label value="Forma de Atuação" style="color:white;font-weight:bold;font-size:14px;" />
					</div>
					<separator height="10px"/>
					<listbox id="listFat" model="@{wAna.listFormaAt}" width="900px" style="border:1px solid #277EB7;">
							<listhead>
								<listheader width="120px" label="Tipo Unidade" style="text-align:center;font-size:11px" />
								<listheader label="Forma de Atuação" style="text-align:left;font-size:11px" />
							</listhead>
							<listitem self="@{each=fat}" value="@{fat}">
								<listcell label="@{fat.tipfat}" style="text-align:center;font-size:11px" />
								<listcell label="@{fat.desfat}" style="text-align:left;font-size:11px"/>
							</listitem>
						</listbox>
				</groupbox>
				<separator height="6px"/>
