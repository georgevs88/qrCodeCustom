							<button label="S2" onClick="wAna.todosS(2)" tooltiptext="Marca todas as Classificações como 'S2' das atividades não exercidas no local."/>
							<separator width="5px"/>
							<button label="Não Conforme" onClick="wAna.todosNaoConforme()" tooltiptext="Marca todas os Status como 'Não Conforme'."/>
						</hbox>
						<listbox id="listAtv" model="@{wAna.listAtvAnalise}" selectedItem="@{wAna.atvAnaliseSel}" width="900px" style="border:1px solid #277EB7;">
