								<listheader width="90px" label="CNAE" style="text-align:center;font-size:11px" />
								<listheader width="50px" label="Risco" style="text-align:center;font-size:11px" />
								<listheader width="85px" label="Exerc. Local" style="text-align:center;font-size:11px" />
								<listheader width="85px" label="Classificação" style="text-align:center;font-size:11px" />
								<listheader width="85px" label="Classificação" style="text-align:center;font-size:11px" />
								<listheader width="135px" label="Status" style="text-align:center;font-size:11px" />
								<listheader label="Descrição" style="text-align:left;font-size:11px" />
							</listhead>
							<listitem self="@{each=atv}" value="@{atv}">
								<listcell label="@{atv.cnae}" style="text-align:center;font-size:11px" />
								<listcell label="@{atv.risco}" style="@{atv.stylerisco}"/>
								<listcell label="@{atv.exelocal}" style="text-align:center;font-size:11px"/>
								<listcell label="@{atv.uso}" style="text-align:center;font-size:11px"/>
								<listcell style="text-align:center;font-size:11px">
									<combobox model="@{wAna.listUsos}" value="@{atv.novouso}" readonly="true" width="60px" onChange="wAna.verificaStatusGeral()">
										<comboitem self="@{each=uso}" label="@{uso.coduso}" value="@{uso.coduso}" style="font-size:11px"/>
									</combobox>
								</listcell>
								<listcell>
									<combobox readonly="true" width="120px" selectedItem="@{atv.staaat}" onChange="wAna.verificaStatusGeral()">
										<comboitem label="Conforme" value="D" style="font-size:11px;color:green"/>
										<comboitem label="Não Conforme" value="I" style="font-size:11px;color:red"/>
									</combobox>
								</listcell>
								<listcell label="@{atv.dcoatv}" style="text-align:left;font-size:11px"/>
							</listitem>
						</listbox>
