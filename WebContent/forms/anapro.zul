							    		<comboitem value="A" label="NÃO SE APLICA" />
										<comboitem value="S" label="SIM" />
										<comboitem value="N" label="NÃO"/>
									</combobox>
					    		</row>
					    	</rows>
					    </grid>
					    <listbox id="listPer" model="@{wAna.colPerguntas}" width="900px" visible="@{wAna.temPerguntas}" style="border:1px solid #277EB7;">
