					    		<column align="right" width="90px"/>
					    		<column width="140px"/>
					    		<column align="right" width="105px"/>
					    		<column width="140px"/>
					    		<column align="right" width="105px"/>
					    		<column />
					    	</columns>
					    	<rows>
					    		<row style="padding:1px;">
					    			<label value="Evento" style="font-size:11px"/>
					    			<textbox width="75px" value="@{wAna.protAnaliseSel.evento}" readonly="true" style="font-size:11px"/>
					    			<label value="Tipo Registro" style="font-size:11px"/>
					    			<textbox width="120px" value="@{wAna.protAnaliseSel.tipvia}" readonly="true" style="font-size:11px"/>
				    				<label value="Situação do Autônomo/MEI" style="font-size:11px"/>
