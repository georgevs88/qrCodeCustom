									</combobox>
								</vbox>
								<vbox>
									<label value="Maior que 5000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.maior_5000m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=uson}" label="@{uson.coduso}"
											value="@{uson.coduso}" style="text-transform:uppercase" tooltiptext="@{uson.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="De 5000m² a 10000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.d5000a10000}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoo}" label="@{usoo.coduso}"
											value="@{usoo.coduso}" style="text-transform:uppercase" tooltiptext="@{usoo.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Maior que 10000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.maior_10000m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=uso}" label="@{uso.coduso}"
											value="@{uso.coduso}" style="text-transform:uppercase" tooltiptext="@{uso.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Qualquer Porte" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.qq_porte}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoq}" label="@{usoq.coduso}"
											value="@{usoq.coduso}" style="text-transform:uppercase" tooltiptext="@{usoq.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="MEI" />
									<combobox id="meiacf" width="60px" readonly="true" selectedItem="@{wAtv.cus.meicus}">
										<comboitem value="S" label='SIM' />
										<comboitem value="N" label='NÃO' />
									</combobox>
								</vbox>
							</row>
<!-- 							<row spans="1,5"> -->
<!-- 								<label value="Última Alteração:" style="font-weight:bold;"/> -->
<!-- 								<textbox value="@{wAtv.cus.usudatalt}" readonly="true" width="200px" style="font-weight:bold;text-align:center;"/> -->
<!-- 							</row> -->
						</rows>
					</grid>
				</div>
			</groupbox>
			<groupbox id="groupRisco">
				<div style="background:#A8A8A8;padding:5px">
					<label value="Riscos" style="color:white;font-weight:bold;font-size:14px;" />
				</div>
				<div align="center">
					<grid width="840px">
						<columns>
							<column width="140px" />
							<column width="140px" align="left" />
							<column width="140px" />
							<column width="140px" align="left" />
							<column width="140px" />
							
						</columns>
						<rows>
							<row>
								<label />
								<vbox>
									<label value="Meio Ambiente" />
									<combobox id="ambccl" readonly="true" width="110px" selectedItem="@{wAtv.ccl.ambccl}" style="text-align:center;font-weight:bold;font-size: 16px;">
										<comboitem value="B" label="BAIXO" />
										<comboitem value="M" label="MÉDIO" />
										<comboitem value="A" label="ALTO" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Vigilância Sanitária" />
									<combobox id="vigccl" readonly="true" width="110px" selectedItem="@{wAtv.ccl.vigccl}" style="text-align:center;font-weight:bold;font-size: 16px;">
										<comboitem value="B" label="BAIXO" />
										<comboitem value="M" label="MÉDIO" />
										<comboitem value="A" label="ALTO" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Bombeiros" />
									<combobox id="bomccl" readonly="true" width="110px" selectedItem="@{wAtv.ccl.bomccl}" style="text-align:center;font-weight:bold;font-size: 16px;">
										<comboitem value="B" label="BAIXO" />
										<comboitem value="M" label="MÉDIO" />
										<comboitem value="A" label="ALTO" />
									</combobox>
								</vbox>
								<vbox>
									<label value="GURB" />
									<combobox id="gurccl" readonly="true" width="110px" selectedItem="@{wAtv.ccl.gurccl}" style="text-align:center;font-weight:bold;font-size: 16px;">
										<comboitem value="B" label="BAIXO" />
										<comboitem value="M" label="MÉDIO" />
										<comboitem value="A" label="ALTO" />
									</combobox>
								</vbox>
							</row>
<!-- 							<row spans="1,5"> -->
<!-- 								<label value="Última Alteração:" style="font-weight:bold;"/> -->
<!-- 								<textbox value="@{wAtv.cus.usudatalt}" readonly="true" width="200px" style="font-weight:bold;text-align:center;"/> -->
<!-- 							</row> -->
						</rows>
					</grid>
				</div>
			</groupbox>
			<div align="center">
				<button label="Salvar Configuração" onClick="wAtv.salvar()" style="width: 200px;font-size: 16px;background-color: #00042A !important;color: white;font-weight: bold;height: 30px;border-radius: 7px !important;"/>
			</div>
		</vbox>
	</hbox>
</window>
</zk>