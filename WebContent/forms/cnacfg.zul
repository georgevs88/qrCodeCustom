											value="@{usof.coduso}" style="text-transform:uppercase" tooltiptext="@{usof.desuso}" />
									</combobox>
								</vbox>
							</row>
							<row>
								<vbox>
									<label value="De 720m² a 2500m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.d720a2500}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usog}" label="@{usog.coduso}"
											value="@{usog.coduso}" style="text-transform:uppercase" tooltiptext="@{usog.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Menor Igual a 1000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.menor_igual_1000m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoh}" label="@{usoh.coduso}"
											value="@{usoh.coduso}" style="text-transform:uppercase" tooltiptext="@{usoh.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Maior que 1000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.maior_1000m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoi}" label="@{usoi.coduso}"
											value="@{usoi.coduso}" style="text-transform:uppercase" tooltiptext="@{usoi.desuso}" />
									</combobox>
								</vbox>
								<vbox>
