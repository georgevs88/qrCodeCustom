<?page title="Configuração CNAE" contentType="text/html;charset=UTF-8"?>
<zk>
<window use="pmcg.imti.view.Cnacfg" id="wAtv" title="Configuração CNAE" width="880px" height="520px" border="normal" shadow="false" position="center"
		mode="overlapped" sizable="false" closable="false" onCreate="wAtv.initComponentes()">
	<separator height="10px"/>
	<hbox width="100%" pack="center" align="center">
		<vbox>
			<grid width="840px">
				<columns>
					<column width="153px" align="right" />
					<column width="140px" />
					<column align="left" />
				</columns>
				<rows>
					<row>
						<label value="CNAE:" style="font-weight:bold;font-size:12px;"/>
						<textbox value="@{wAtv.cnae}" width="90px" onOK="wAtv.pesquisar()" style="font-weight:bold;font-size:12px;text-align:center;"/>
						<button label="Pesquisar" width="120px" image="/images/imgBuscar.png" style="border:1px solid #3083BA;background:white" onClick="wAtv.pesquisar()"/>
					</row>
					<row spans="1,2">
						<div align="right">
							<label value="Código:" style="font-weight:bold;font-size:14px;"/>
						</div>
						<label value="@{wAtv.cus.codcus}" style="font-weight:bold;font-size:14px;color:#FF6C00;"/>
					</row>
					<row spans="1,2">
						<div align="right">
							<label value="Descrição:" style="font-weight:bold;font-size:14px;"/>
						</div>
						<label value="@{wAtv.cus.nomcus}" style="font-weight:bold;font-size:14px;color:#FF6C00;"/>
					</row>
				</rows>
			</grid>
			<groupbox id="groupUso">
				<div style="background:#A8A8A8;padding:5px">
					<label value="Usos por Porte" style="color:white;font-weight:bold;font-size:14px;" />
				</div>
				<div align="center">
					<grid width="840px">
						<columns>
							<column width="140px" align="left" />
							<column width="140px" />
							<column width="140px" align="left" />
							<column width="140px" />
							<column width="140px" align="left" />
							<column width="140px" />
						</columns>
						<rows>
							<row>
								<vbox>
									<label value="Menor Igual a 360m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true" value="@{wAtv.cus.menor_igual_360m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoa}" label="@{usoa.coduso}"
											value="@{usoa.coduso}" style="text-transform:uppercase" tooltiptext="@{usoa.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="De 360m² a 1000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.d360a1000}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usob}" label="@{usob.coduso}"
											value="@{usob.coduso}" style="text-transform:uppercase" tooltiptext="@{usob.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Menor Igual a 500m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.menor_igual_500m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoc}" label="@{usoc.coduso}"
											value="@{usoc.coduso}" style="text-transform:uppercase" tooltiptext="@{usoc.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="De 500m² a 1000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.d500a1000}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usod}" label="@{usod.coduso}"
											value="@{usod.coduso}" style="text-transform:uppercase" tooltiptext="@{usod.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Menor Igual a 720m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.menor_igual_720m}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usoe}" label="@{usoe.coduso}"
											value="@{usoe.coduso}" style="text-transform:uppercase" tooltiptext="@{usoe.desuso}" />
									</combobox>
								</vbox>
								<vbox>
									<label value="Maior que 720m²" />
