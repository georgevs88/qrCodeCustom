									</combobox>
								</vbox>
								<vbox>
									<label value="De 2500m² a 5000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
										value="@{wAtv.cus.d2500a5000}" style="text-transform:uppercase" width="60px">
										<comboitem self="@{each=usol}" label="@{usol.coduso}"
											value="@{usol.coduso}" style="text-transform:uppercase" tooltiptext="@{usol.desuso}" />
									</combobox>
								</vbox>
							</row>
							<row>
								<vbox>
									<label value="Menor Igual a 5000m²" />
									<combobox model="@{wAtv.listUsos}" readonly="true"
