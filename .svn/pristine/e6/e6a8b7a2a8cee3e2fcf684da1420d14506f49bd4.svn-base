<?page cacheable="false" language="xul/html" zscriptLanguage="Java" contentType="text/html;charset=UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zul" xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window use="pmcg.imti.view.Cefcon" id="wCon" title="Consulta Protocolo EF" width="400px" height="295px" border="normal" shadow="false" position="center"
		mode="overlapped" sizable="false" closable="true" onCreate="wCon.initComponentes()" contentStyle="overflow:auto;">
		<style src="/ext/integrarweb_ext.css"></style>
		<grid style="margin:15px">
	    	<columns>
	    		<column align="right" width="120px"/>
	    		<column />
	    	</columns>
	    	<rows>
	    		<row style="padding:1px;" spans="1,5">
	    			<label value="CNPJ:" style="font-size:16px;"/>
					<textbox id="cnpj" value="@{wCon.cnpj}" style="text-transform:uppercase;font-size:16px;text-align:center"/>
				</row>
			</rows>
		</grid>
		<div align="center">
			<a onClick="wCon.consultar()">
				<h:button class="btn btn-lg btn-default">
					<span class="glyphicon glyphicon-search"></span>
					Consultar
				</h:button>
			</a>
		</div>
		<grid style="margin:15px" visible="@{wCon.visibleDados}">
			<columns>
	    		<column align="right" width="150px"/>
	    		<column />
	    	</columns>
	    	<rows>
	    		<row style="padding:1px;" spans="1,5">
	    			<label value="CNPJ:" style="font-size:16px;"/>
					<label value="@{wCon.dados.cpfcnpj}" style="text-transform:uppercase;font-size:16px;font-weight:bold;color:red;"/>
				</row>
				<row style="padding:1px;" spans="1,5">
	    			<label value="Protocolo:" style="font-size:16px;"/>
					<label value="@{wCon.dados.protocolo}" style="text-transform:uppercase;font-size:16px;font-weight:bold;color:red;"/>
				</row>
				<row style="padding:1px;" spans="1,5">
	    			<label value="Data Conciliação:" style="font-size:16px;"/>
					<label value="@{wCon.dados.data}" style="text-transform:uppercase;font-size:16px;font-weight:bold;color:red;"/>
				</row>
			</rows>
		</grid>
		<style src="/ext/integrarweb_ext.css"></style>
		<style src="/css/bootstrap.min.css"></style>
		<style src="/css/componentes.css"></style>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    	<script src="/js/bootstrap.min.js"></script>
	</window>
</zk>
