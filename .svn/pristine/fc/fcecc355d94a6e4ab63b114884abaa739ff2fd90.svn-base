<?page title="Configurações Integrar" cacheable="false" language="xul/html" zscriptLanguage="Java" contentType="text/html;charset=UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zul" xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window use="pmcg.imti.view.Cfgcad" id="wCfg" title="Configurações Integrar" width="400px" border="normal" shadow="false" position="center"
		mode="overlapped" sizable="false" closable="true" onCreate="wCfg.initComponentes()"  contentStyle="overflow:auto;">
		<style src="/ext/integrarweb_ext.css"></style>
		<div style="padding:10px">
			<groupbox>
				<caption label="Controles WS"/>
				<grid>
			    	<columns>
			    		<column align="right" width="160px"/>
			    		<column />
			    	</columns>
			    	<rows>
			    		<row>
			    			<label value="Grava Viabilidade:" style="font-size:11px;"/>
							<combobox readonly="true" selectedItem="@{wCfg.grava_via}" width="70px">
								<comboitem value="1" label="Ativo" />
								<comboitem value="0" label="Inativo"/>
							</combobox>
			    		</row>
			    		<row>
			    			<label value="Responde Viabilidade:" style="font-size:11px;"/>
							<combobox readonly="true" selectedItem="@{wCfg.resp_via}" width="70px">
								<comboitem value="1" label="Ativo" />
								<comboitem value="0" label="Inativo"/>
							</combobox>
			    		</row>
			    		<row>
			    			<label value="Grava Empreendimento" style="font-size:11px;"/>
							<combobox readonly="true" selectedItem="@{wCfg.grava_emp}" width="70px">
								<comboitem value="1" label="Ativo" />
								<comboitem value="0" label="Inativo"/>
							</combobox>
			    		</row>
			    		<row>
			    			<label value="Responde Empreendimento" style="font-size:11px;"/>
							<combobox readonly="true" selectedItem="@{wCfg.resp_emp}" width="70px">
								<comboitem value="1" label="Ativo" />
								<comboitem value="0" label="Inativo"/>
							</combobox>
			    		</row>
			    		<row>
			    			<label value="Máximo Registro VIA" style="font-size:11px;"/>
							<intbox value="@{wCfg.maxvia}" width="66px"/>
			    		</row>
			    		<row>
			    			<label value="Máximo Registro EPD" style="font-size:11px;"/>
							<intbox value="@{wCfg.maxepd}" width="66px"/>
			    		</row>
			    	</rows>
			    </grid>
		    </groupbox>
		</div>
		
	</window>
</zk>
