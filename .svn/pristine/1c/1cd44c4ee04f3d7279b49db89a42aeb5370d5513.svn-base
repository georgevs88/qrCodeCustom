<?page title="Notificação Auto de Infração" cacheable="false" language="xul/html" zscriptLanguage="Java" contentType="text/html;charset=UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zul" xmlns:h="http://www.w3.org/1999/xhtml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.zkoss.org/2005/zul http://www.zkoss.org/2005/zul/zul.xsd">

	<window use="pmcg.imti.view.Aifcad" id="wAif" title="Cadastro de Notificação Auto de Infração" width="900px" height="640px" border="normal" shadow="false" position="center"
		mode="modal" sizable="false" closable="true" onCreate="wAif.initComponentes()" contentStyle="overflow:auto;">
		<vbox>
			<separator height="10px"/>
			<grid>
		    	<columns>
		    		<column align="right" width="155px"/>
		    		<column align="left" width="160px"/>
		    		<column align="right" width="120px"/>
		    		<column align="left" width="160px"/>
		    		<column align="right" width="100px"/>
		    		<column align="left" width="185px"/>
		    	</columns>
		    	<rows>
		    		<row>
		    			<label value="Notificação:"/>
						<intbox id="notaif" value="@{wAif.aifVO.notaif}" width="145px" style="text-align:center;font-size:13px;font-weight:bold;"/>
						<label value="Auto de Infração:"/>
		    			<intbox id="aifaif" value="@{wAif.aifVO.aifaif}" width="145px" style="text-align:center;font-size:13px;font-weight:bold;"/>
					</row>
		    		<row>
		    			<label value="Dt Rec. Notif.:"/>
						<datebox id="drnaif" value="@{wAif.aifVO.drnaif}" width="149px" style="text-align:center;"/>
						<label value="Dt Rec. Auto de Inf.:"/>
						<datebox id="draaif" value="@{wAif.aifVO.draaif}" width="149px" style="text-align:center;"/>
					</row>
					<row>
		    			<label value="Apresentou defesa:"/>
						<combobox id="apdaif" readonly="true" width="149px" selectedItem="@{wAif.aifVO.apdaif}" style="text-align:center;">
							<comboitem value="S" label="SIM"/>
							<comboitem value="N" label="NÃO"/>
						</combobox>
						<label value="Prazo final defesa:"/>
						<datebox id="pfdaif" value="@{wAif.aifVO.pfdaif}" width="149px" style="text-align:center;"/>
						<label value="AR Retornou:"/>
						<combobox id="arraif" readonly="true" width="149px" selectedItem="@{wAif.aifVO.arraif}" style="text-align:center;">
							<comboitem value="S" label="SIM"/>
							<comboitem value="N" label="NÃO"/>
						</combobox>
					</row>
					<row spans="1,7">
		    			<label value="Nome"/>
						<textbox id="nomaif" value="@{wAif.aifVO.nomaif}" width="686px" style="font-weight:bold;font-size: 13px;text-transform:uppercase;"/>
					</row>
					<row spans="1,7">
		    			<label value="Atividade:"/>
						<textbox id="atvaif" value="@{wAif.aifVO.atvaif}" width="686px" rows="2" style="text-transform:uppercase;"/>
					</row>
					<row spans="1,7">
		    			<label value="Legislação:"/>
						<textbox id="legaif" value="@{wAif.aifVO.legaif}" width="686px" rows="2" style="text-transform:uppercase;"/>
					</row>
					<row spans="1,7">
		    			<label value="Tipos de Notificações:"/>
						<textbox id="tpnaif" value="@{wAif.aifVO.tpnaif}" width="686px" style="text-transform:uppercase;"/>
					</row>
					<row>
		    			<label value="Relatório Técnico:"/>
						<textbox id="relaif" value="@{wAif.aifVO.relaif}" width="145px" style="text-transform:uppercase;"/>
						<label value="Laudo de Vistoria:"/>
						<textbox id="lauaif" value="@{wAif.aifVO.lauaif}" width="145px" style="text-transform:uppercase;"/>
					</row>
					<row spans="1,7">
		    			<label value="Endereço:"/>
						<textbox id="edraif" value="@{wAif.aifVO.edraif}" width="686px" style="text-transform:uppercase;"/>
					</row>
					<row>
		    			<label value="Bairro:"/>
						<textbox id="baiaif" value="@{wAif.aifVO.baiaif}" width="145px" style="text-transform:uppercase;"/>
						<label value="Região Urbana:"/>
		    			<combobox id="regaif" readonly="true" width="149px" selectedItem="@{wAif.aifVO.regaif}">
		    				<comboitem value="ANHANDUI" label="ANHANDUI" />
							<comboitem value="ANHANDUIZINHO" label="ANHANDUIZINHO" />
							<comboitem value="BANDEIRA" label="BANDEIRA" />
							<comboitem value="CENTRO" label="CENTRO" />
							<comboitem value="IMBIRUSSU" label="IMBIRUSSU" />
							<comboitem value="LAGOA" label="LAGOA" />
							<comboitem value="PROSA" label="PROSA" />
							<comboitem value="ROCHEDINHO" label="ROCHEDINHO" />
							<comboitem value="SEGREDO" label="SEGREDO" />
							<comboitem value="ZONA RURAL" label="ZONA RURAL" />
						</combobox>
						<label value="Agente Fiscal:"/>
						<textbox id="ageaif" value="@{wAif.aifVO.ageaif}" width="149px" style="text-transform:uppercase;"/>
					</row>
					<row>
		    			<label value="Processo nr:"/>
						<textbox id="proaif" value="@{wAif.aifVO.proaif}" width="145px" style="text-transform:uppercase;"/>
						<label value="Formalizado em:"/>
		    			<datebox id="dtfaif" value="@{wAif.aifVO.dtfaif}" width="149px" style="text-align:center;"/>
					</row>
					<row spans="1,7">
		    			<label value="Observações:"/>
						<textbox id="obsaif" value="@{wAif.aifVO.obsaif}" width="686px" rows="2" style="text-transform:uppercase;"/>
					</row>
					<row>
		    			<label value="Situação Final Proc.:"/>
						<textbox id="sitaif" value="@{wAif.aifVO.sitaif}" width="145px" style="text-transform:uppercase;"/>
						<label value="Data Lavrado:"/>
		    			<datebox id="dtlaif" value="@{wAif.aifVO.dtlaif}" width="149px" style="text-align:center;"/>
		    			<label value="Reincidencia:"/>
						<combobox id="reiaif" readonly="true" width="149px" selectedItem="@{wAif.aifVO.reiaif}" style="text-align:center;">
							<comboitem value="S" label="SIM"/>
							<comboitem value="N" label="NÃO"/>
						</combobox>
					</row>
					<row>
						<label value="Cadastrado p/ publicar:"/>
						<combobox id="cdpaif" readonly="true" width="149px" selectedItem="@{wAif.aifVO.cdpaif}" style="text-align:center;">
							<comboitem value="S" label="SIM"/>
							<comboitem value="N" label="NÃO"/>
						</combobox>
					</row>
				</rows>
		    </grid>
		</vbox>
	</window>
</zk>
