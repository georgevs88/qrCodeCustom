package pmcg.fcn.domain;

import com.thoughtworks.xstream.annotations.XStreamAlias;
import com.thoughtworks.xstream.annotations.XStreamAsAttribute;

@XStreamAlias(value = "tipoLogradouro")
public class TloVO {
	@XStreamAlias("codigoFRB") @XStreamAsAttribute 
	private String codigoFRB; //codigoFRB - EMPREENDIMENTO
	@XStreamAlias("codigo") @XStreamAsAttribute 
	private String codigo; //VIABILIDADE
	

	public String getCodigoFRB() {
		return codigoFRB;
	}

	public void setCodigoFRB(String codigoFRB) {
		this.codigoFRB = codigoFRB;
	}

	public String getCodigo() {
		return codigo;
	}

	public void setCodigo(String codigo) {
		this.codigo = codigo;
	}
}
