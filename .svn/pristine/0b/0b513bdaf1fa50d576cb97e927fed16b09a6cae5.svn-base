package pmcg.fcn.siat.domain;

/**
 * Representa os 03 tipos de Pessoa que uma Natureza Jurídica pode pertencer
 * (Pessoa Física, Pessoa Jurídica ou Pessoa Física Equiparada a Jurídica).
 * @author George do Valle
 * @version 1.0
 * @created 31-out-2007 08:09:39
 */
public enum TipoPessoa {
	
	PF, 	/* Pessoa Fisica 			*/
	PJ,		/* Pessoa Juridica			*/
	PFE;	/* Pessoa Fisica Equiparada */
	
	public String getCodigo(){
		return toString();
	}
}