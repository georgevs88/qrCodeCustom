		this.anxarq = anxarq; 
	}
 
	/**
	* Tipo mime do arquivo
	**/
	public void setTiparq(String tiparq){
		this.tiparq = tiparq; 
	}
 
	/**
	* nome do arquivo
	**/
	public void setNomarq(String nomarq){
		this.nomarq = nomarq; 
	}
 
	/**
	* Observacao para o arquivo
	**/
	public void setObsarq(String obsarq){
		this.obsarq = obsarq; 
	}

}