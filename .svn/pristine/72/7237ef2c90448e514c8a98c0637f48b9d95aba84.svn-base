package pmcg.imti.domain; 

public class ArqanxVO{
	private Long seqarq;
	private Integer codsis;
	private byte[] anxarq;
	private String tiparq;
	private String nomarq;
	private String obsarq;

	/**
	* Codigo do arquivo
	**/
	public Long getSeqarq(){
		return this.seqarq; 
	}
 
	/**
	* Codigo do sistema
	**/
	public Integer getCodsis(){
		return this.codsis; 
	}
 
	/**
	* Arquivo em bytes
	**/
	public byte[] getAnxarq(){
		return this.anxarq; 
	}
 
	/**
	* Tipo mime do arquivo
	**/
	public String getTiparq(){
		return this.tiparq; 
	}
 
	/**
	* nome do arquivo
	**/
	public String getNomarq(){
		return this.nomarq; 
	}
 
	/**
	* Observacao para o arquivo
	**/
	public String getObsarq(){
		return this.obsarq; 
	}


	/**
	* Codigo do arquivo
	**/
	public void setSeqarq(Long seqarq){
		this.seqarq = seqarq; 
	}
 
	/**
	* Codigo do sistema
	**/
