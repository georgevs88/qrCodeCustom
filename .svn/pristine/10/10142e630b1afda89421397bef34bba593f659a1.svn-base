package pmcg.fcn.siat.domain;

/**
 * Representa a Direcao da Numeraçao do Endereço no Logradouro. Ex: Norte, Sul,
 * Leste, Oeste
 * @author Tais Santana
 * @version 1.0
 * @created 30-jun-2010 14:56:53
 */
public enum Direcao {

	NE /* Nenhum */ ,
	N /* Norte */ ,
	S /* Sul */ ,
	L /* Leste */ ,
	O /* Oeste */ ;

	public String getCodigo() {
		return this.toString();
	}
}