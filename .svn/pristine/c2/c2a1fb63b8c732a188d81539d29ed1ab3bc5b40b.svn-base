package pmcg.imti.domain;

import java.util.Date;

import gerador.annotation.ZKEntity;
import gerador.annotation.ZKField;
import gerador.annotation.ZKId;

@ZKEntity(label = "CNAE Classe", sequence = "cnae_classe_cnae_id_seq")
public class CclVO {
	@ZKId
	@ZKField(label = "Código", artigo = "o", readonly = true, maxsize = 80, tooltip = "Código incremental!")
	private Double iddccl; //ID
	private Double uniccl; //CODIGO_UNIDADE
	private Double cnaccl; //CNAE_ID
	private String rioccl; //RISCO_OLD
	private String rinccl; //RISCO_NEW
	private String usualt; //USUALT
	private Date datalt; //DATALT
	public Double getIddccl() {
		return iddccl;
	}
	public void setIddccl(Double iddccl) {
		this.iddccl = iddccl;
	}
	public Double getUniccl() {
		return uniccl;
	}
	public void setUniccl(Double uniccl) {
		this.uniccl = uniccl;
	}
	public Double getCnaccl() {
		return cnaccl;
	}
	public void setCnaccl(Double cnaccl) {
		this.cnaccl = cnaccl;
	}
	public String getRioccl() {
		return rioccl;
	}
	public void setRioccl(String rioccl) {
		this.rioccl = rioccl;
	}
	public String getRinccl() {
		return rinccl;
	}
	public void setRinccl(String rinccl) {
		this.rinccl = rinccl;
	}
	public String getUsualt() {
		return usualt;
	}
	public void setUsualt(String usualt) {
		this.usualt = usualt;
	}
	public Date getDatalt() {
		return datalt;
	}
	public void setDatalt(Date datalt) {
		this.datalt = datalt;
	}

}
