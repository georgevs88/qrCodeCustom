package pmcg.fcn.siat.domain;

/**
 * Classe que representa os tipos de Riscos do Cadastro Econômico. Informação é
 * enviada pelo sistema Integrar.
 * @author George do Valle
 * @version 1.0
 * @created 02-ago-2010 10:57:11
 */
public enum RiscoAlvara {
	/**
	 * Indica que o Cadastro Econômico é de risco Alto.
	 */
	A /* Alto */ ,
	/**
	 * Indica que o Cadastro Econômico é de risco Médio.
	 */
	M /* Médio */ ,
	/**
	 * Indica que o Cadastro Econômico é de risco Baixo.
	 */
	B /* Baixo */ ;

	public String getCodigo() {
		return this.toString();
	}
}