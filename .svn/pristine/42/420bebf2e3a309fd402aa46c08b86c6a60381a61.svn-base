package pmcg.fcn.siat.domain.ws;

import java.math.BigDecimal;
import java.util.List;

public class EntradaDebitosCadastro {
	
	private Integer qtdParcelasAbertas;
	private BigDecimal valorParcelasAbertas;
	private List<EntradaParcelaCalculada> parcelas;
	
	public EntradaDebitosCadastro() {}
	
	public EntradaDebitosCadastro(Integer qtdParcelasAbertas, BigDecimal valorParcelasAbertas, List<EntradaParcelaCalculada> parcelas) {
		setQtdParcelasAbertas(qtdParcelasAbertas);
		setValorParcelasAbertas(valorParcelasAbertas);
		setParcelas(parcelas);
	}
	
	
	public Integer getQtdParcelasAbertas() {
		return qtdParcelasAbertas;
	}

	public void setQtdParcelasAbertas(Integer qtdParcelasAbertas) {
		this.qtdParcelasAbertas = qtdParcelasAbertas;
	}

	public BigDecimal getValorParcelasAbertas() {
		return valorParcelasAbertas;
	}

	public void setValorParcelasAbertas(BigDecimal valorParcelasAbertas) {
		this.valorParcelasAbertas = valorParcelasAbertas;
	}

	public List<EntradaParcelaCalculada> getParcelas() {
		return parcelas;
	}

	public void setParcelas(List<EntradaParcelaCalculada> parcelas) {
		this.parcelas = parcelas;
	}
}
